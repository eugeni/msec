#: ../src/msec/config.py:47
msgid "No changes in system files"
msgstr "Süsteemsetes failides muutusi pole"

#: ../src/msec/config.py:60
msgid "Disabled"
msgstr "Keelatud"

#: ../src/msec/config.py:211 ../src/msec/config.py:411
